; start main program
	TSTO #18
	BOV stack_overflow_error
	ADDSP #14
;  ************************************************************
;  *           CONSTRUCTION DES VTABLES DES CLASSES           *
;  ************************************************************
; Construction de la VTable de la classe Object
	LOAD #null, R0	;     // On garde Dans R0 l'addr de la classe Object pour eviter de le loader a chaque fois !! 
	STORE R0, 1(GB)
	LOAD code.Object.equals, R1	;     // On garde Dans R1 le label 'code.Object.equals' pour la meme raison 
	STORE R1, 2(GB)
; Construction de la VTable de A
	LEA 1(GB), R2
	STORE R2, 3(GB)
	STORE R1, 4(GB)
	LOAD code.A.initX, R2
	STORE R2, 5(GB)
	LOAD code.A.getX, R2
	STORE R2, 6(GB)
; Construction de la VTable de B
	LEA 3(GB), R2
	STORE R2, 7(GB)
	STORE R1, 8(GB)
	LOAD code.B.initX, R2
	STORE R2, 9(GB)
	LOAD code.A.getX, R2
	STORE R2, 10(GB)
; Variable Declarations : 
	NEW #2, R2
	BOV tas_plein
	LEA 7(GB), R3
	STORE R3, 0(R2)
	PUSH R2
	BSR init.B
	POP R2
	STORE R2, 13(GB)
	NEW #2, R2
	BOV tas_plein
	LEA 7(GB), R3
	STORE R3, 0(R2)
	PUSH R2
	BSR init.B
	POP R2
	STORE R2, 14(GB)
; Beginning of main instructions:
	LOAD 13(GB), R2
	CMP #null, R2
	BEQ not_instance_B_46_8
	LEA 7(GB), R4
	LOAD 0(R2), R2
start_instanceof_B_46_8:
	CMP R4, R2
	BEQ is_instance_B_46_8
	LOAD 0(R2), R2
	CMP #null, R2
	BNE start_instanceof_B_46_8
not_instance_B_46_8:
	LOAD #0, R3
	BRA end_instanceof_B_46_8
is_instance_B_46_8:
	LOAD #1, R3
end_instanceof_B_46_8:
	CMP #0, R3
	BEQ E_Sinon.1
	WSTR "true"
	WNL
	BRA E_Fin_if.1
E_Sinon.1:
E_Fin_if.1:
	ADDSP #2
	LOAD 14(GB), R2
	STORE R2, 0(SP)
	LOAD #10, R0
	STORE R0, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement.null
	LOAD 0(R2), R2
	BSR 2(R2)
	SUBSP #2
	ADDSP #1
	LOAD 14(GB), R2
	STORE R2, 0(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement.null
	LOAD 0(R2), R2
	BSR 3(R2)
	SUBSP #1
	STORE R0, 12(GB)
	LOAD 12(GB), R2
	LOAD #3, R1
	CMP #0, R1
	BEQ div_zero_error
	REM #3, R2
	STORE R2, 11(GB)
	ADDSP #1
	LOAD 14(GB), R2
	STORE R2, 0(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement.null
	LOAD 0(R2), R2
	BSR 3(R2)
	SUBSP #1
	LOAD R0, R1
	WINT
	WNL
	LOAD 11(GB), R2
	LOAD R2, R1
	WINT
	WNL
	HALT
; end main program
;  ************************************************************
;  *  GENERATION DES INITS ET DES BODY METHODES DES CLASSES   *
;  ************************************************************
init.A:
	TSTO #0
	BOV stack_overflow_error
	LOAD -2(LB), R2
	LOAD #0, R0
	STORE R0, 1(R2)
	LOAD -2(LB), R2
	RTS
init.B:
	TSTO #3
	BOV stack_overflow_error
	LOAD -2(LB), R2
	PUSH R2
	BSR init.A
	POP R2
	LOAD -2(LB), R2
	RTS
code.Object.equals:
	LOAD -2(LB), R0
	CMP -3(LB), R0
	SEQ R1
	RTS
code.A.initX:
	TSTO #2
	BOV stack_overflow_error
	PUSH R2
	PUSH R3
	LOAD -3(LB), R2
	LOAD -2(LB), R3
	CMP #null, R3
	BEQ dereferencement.null
	STORE R2, 1(R3)
fin.A.initX:
	POP R3
	POP R2
	RTS
code.A.getX:
	TSTO #4
	BOV stack_overflow_error
	PUSH R2
	PUSH R3
	PUSH R4
	PUSH R5
	LOAD -2(LB), R3
	LOAD 1(R3), R2
	LOAD R2, R0
	BRA fin.A.getX
	BRA absence_return_error
fin.A.getX:
	POP R5
	POP R4
	POP R3
	POP R2
	RTS
code.B.initX:
	TSTO #5
	BOV stack_overflow_error
	PUSH R2
	ADDSP #2
	LOAD -2(LB), R2
	STORE R2, 0(SP)
	LOAD -3(LB), R2
	MUL #2, R2
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	CMP #null, R2
	BEQ dereferencement.null
	BSR code.A.initX
	SUBSP #2
fin.B.initX:
	POP R2
	RTS
;  ************************************************************
;  * AJOUT DES LABELS ERREURS DEMANDÃ‰ES PAR LE PROGRAMME DECA *
;  ************************************************************
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
div_zero_error:
	WSTR "Erreur : division par 0"
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : le tas est plein, impossible d'allouer de la memoire"
	WNL
	ERROR
absence_return_error:
	WSTR "Erreur : Absence de return dans le corps d'une methode "
	WNL
	ERROR
dereferencement.null:
	WSTR "Erreur : Dereferencement null"
	WNL
	ERROR
