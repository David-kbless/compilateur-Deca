{
    int i;
    int x;

    i = 0;
    x = 0;

    while (i < 100000) {
        if (i % 2 == 0) {
            if (i % 3 == 0) {
                x = x + 1;
            } else {
                x = x - 1;
            }
        } else {
            if (i % 5 == 0) {
                x = x + 2;
            }
        }
        i = i + 1;
    }

    println(x);
}
