    void m(){
        println(" méthode m de A ");
    }

}

class B extends A{
    void m(){
        println(" méthode m de B ");
    }
}

class C extends B{
    void m(){
        println(" méthode m de C ");
    }
}


{
    // déclarations des objets
    A a;
    B b;
    C c;

    // création des objets
    a = new A();
    b = new B();
    c = new C();

    // erreurs de conversion
    c = (C) (a);
